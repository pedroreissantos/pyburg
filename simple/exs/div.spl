program	# Print positive integer divisors
  n = 0;
  d = 0;
  print 'Positive integer divisors calculation.';
  print 'Enter a number: ';
  read n;
  if (n > 0) {
    i = 2;
    while (i <= n/2) {
      if (n % i == 0) {
        print i;	# "n" divides by "i"
        d = d + 1;
      }
      i = i + 1;
    }
    if (d == 0) {
      print 'is prime.';
    }
  }
end
